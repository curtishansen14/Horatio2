@model IEnumerable<Horatio_2._0.Models.UserQuest>

@{
    ViewBag.Title = "Index";
}
<br />
<br />
<br />

<h2>Profile</h2>
    <table class="table">
        <tr>
 
            <th>
                @Html.DisplayNameFor(model => model.Quest.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isComplete)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Target)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.Quest.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isComplete)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Target)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.UserQuestID }) |
                @Html.ActionLink("Details", "Details", new { id=item.UserQuestID }) |
   
            </td>
        </tr>
    }
    
    </table>

